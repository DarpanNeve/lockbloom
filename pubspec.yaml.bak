name: lockbloom
description: "A new Flutter project."
publish_to: 'none'
version: 1.0.2+3

environment:
  sdk: ^3.7.0

dependencies:
  flutter:
    sdk: flutter
  cupertino_icons: ^1.0.8
  get: ^4.7.2
  flutter_screenutil: ^5.9.3
  flutter_secure_storage: ^9.2.4
  encrypt: ^5.0.3
  pointycastle: ^3.9.1
  local_auth: ^2.3.0
  flutter_svg: ^2.2.0
  lottie: ^3.3.1
  uuid: ^4.5.1
  intl: ^0.20.2
  share_plus: ^11.1.0
  file_picker: ^10.3.2
  json_annotation: ^4.9.0
  freezed_annotation: ^3.1.0
  shared_preferences: ^2.5.3
  fluttertoast: ^8.2.6
  upgrader: ^11.5.0
  shorebird_code_push: ^2.0.4
  firebase_core: ^4.0.0
  firebase_analytics: ^12.0.0
  firebase_crashlytics: ^5.0.0
  firebase_performance: ^0.11.0
  flutter_launcher_icons: ^0.14.4
  flutter_native_splash: ^2.4.6
  rename_app: ^1.6.5

flutter_launcher_icons:
  android: true
  ios: true
  image_path: "assets/images/short_icon.png" # Your app icon
  remove_alpha_ios: true
  background_color: "#F2E7CB" # background color for the icon
  adaptive_icon_background: "#F2E7CB" # background color for the adaptive icon

flutter_native_splash:
  android: true
  ios: true
  color: "#ffffff"
  image: assets/images/icon.png # splash image (centered)
  android_12:
    image: assets/images/icon.png # splash image for Android 12 (centered)


dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^5.0.0

flutter:
  uses-material-design: true
  assets:
    - assets/
    - shorebird.yaml